    "jest-dom": "^3.5.0",
    "node-fetch": "^2.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-modal": "^3.8.1",
    "react-redux": "^7.0.3",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.13.2",
    "standard": "^12.0.1",
    "uglify-js": "^3.6.0",
    "uuid": "^3.3.2",
    "watchify": "^3.11.1"
