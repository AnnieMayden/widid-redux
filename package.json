      "transform-es5-property-mutators",
      "transform-exponentiation-operator",
      "transform-function-bind",
      "transform-object-rest-spread",
      "transform-proto-to-assign",
      "transform-react-display-name",
      "transform-react-jsx"
    ]
  },
  "standard": {
    "ignore": [
      "src/polyfill.js"
    ]
  },
  "devDependencies": {
    "enzyme": "^2.9.1"
  }
