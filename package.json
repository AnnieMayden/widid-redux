  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect  ./node_modules/jest/bin/jest.js --runInBand",
    "flow": "flow status --include-warnings",
    "lint": "standard '**/*.js' | snazzy",
